{
  "name": "@livepeer-frameworks/player-svelte",
  "version": "0.1.0",
  "type": "module",
  "description": "Svelte 5 components for FrameWorks streaming player",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./player.css": "./src/player.css"
  },
  "scripts": {
    "build": "svelte-package -i src -o dist",
    "build:watch": "svelte-package -i src -o dist --watch",
    "type-check": "svelte-check"
  },
  "dependencies": {
    "@livepeer-frameworks/player-core": "workspace:*",
    "bits-ui": "^2.15.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "svelte": "^5.48.2",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.2",
    "vite": "^7.3.1"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "streaming",
    "player",
    "video"
  ],
  "author": "Livepeer FrameWorks",
  "license": "Unlicense"
}
